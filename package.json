{
  "name": "@wozien/kun-ui",
  "version": "0.1.1",
  "description": "UI compoents library base vue3.0",
  "main": "dist/kun-ui.umd.js",
  "module": "dist/kun-ui.mjs",
  "keywords": [
    "vue component",
    "vite",
    "typescript"
  ],
  "author": "wozien",
  "repository": {
    "url": "https://github.com/wozien/kun-ui"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --force",
    "build": "pnpm build:all && pnpm build:components",
    "build:all": "vite build",
    "build:components": "esno ./build/buildComponent.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --fix --ext .ts,.tsx,.vue src",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue}\"",
    "doc:dev": "vitepress dev docs",
    "doc:build": "vitepress build docs",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "pnpm lint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@iconify-json/ic": "^1.1.9",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "2.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.24.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "esno": "^0.16.3",
    "happy-dom": "6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "unocss": "^0.45.22",
    "vite": "3.0.9",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "0.21.1"
  },
  "dependencies": {
    "vue": "^3.2.39"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  }
}
